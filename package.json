{
  "name": "logistics-wizard",
  "version": "0.1.0",
  "description": "Front end UI for Logistics Wizard demo",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "test:dev-verbose": "npm run test -- --verbose --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "storybook": "start-storybook -p 9001",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "check-coverage": "nyc check-coverage --statements 70 --branches 50 --functions 50 --lines 70"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node_modules/.bin/nyc node --harmony-proxies ./node_modules/.bin/ava",
      "env": {
        "NODE_ENV": "test",
        "NODE_PATH": "src",
        "CONTROLLER_SERVICE": "https://fake-controller.net"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM-Cloud/logistics-wizard-webui.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "rimraf": "2.6.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "better-npm-run": "0.0.10",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "cssnano": "3.10.0",
    "debug": "2.6.9",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "font-awesome": "4.7.0",
    "fs-extra": "0.30.0",
    "google-map-react": "0.19.1",
    "history": "2.1.2",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.6.5",
    "ip": "1.1.5",
    "json-loader": "0.5.7",
    "koa": "2.4.1",
    "koa-connect-history-api-fallback": "0.3.1",
    "koa-convert": "1.2.0",
    "koa-proxy": "0.6.0",
    "koa-static": "3.0.0",
    "material-ui": "0.16.7",
    "moment": "2.20.1",
    "node-sass": "3.13.1",
    "normalize.css": "4.2.0",
    "postcss-loader": "0.9.1",
    "react": "15.6.2",
    "react-container-query": "0.5.8",
    "react-dom": "15.6.2",
    "react-fa": "4.2.0",
    "react-ga": "2.4.0",
    "react-redux": "4.4.8",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.8",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.7.2",
    "redux-saga": "0.11.1",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.2",
    "url-loader": "0.5.9",
    "webpack": "1.15.0",
    "whatwg-fetch": "2.0.3",
    "yargs": "4.8.1"
  },
  "devDependencies": {
    "@kadira/react-storybook-decorator-centered": "1.1.2",
    "@kadira/storybook": "2.35.3",
    "ava": "0.15.2",
    "babel-eslint": "6.1.2",
    "coveralls": "2.13.3",
    "enzyme": "2.9.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "faker": "3.1.0",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "9.12.0",
    "mock-css-modules": "1.0.0",
    "nock": "8.2.2",
    "nodemon": "1.14.9",
    "nyc": "12.0.2",
    "react-addons-test-utils": "15.6.2",
    "redbox-react": "1.5.0",
    "redux-ava": "2.2.0",
    "sinon": "1.17.7",
    "webpack-dev-middleware": "1.12.2",
    "webpack-hot-middleware": "2.21.0"
  },
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "require": [
      "babel-register",
      "./config/ava.config.js"
    ],
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!src/**/*.spec.js",
      "!src/**/*.story.js"
    ]
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.test.js",
      "src/**/*.story.js",
      "src/routes/**/index.js",
      "src/store/*"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
